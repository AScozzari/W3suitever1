{
  "name": "@w3suite/reverse-proxy",
  "version": "1.0.0",
  "description": "Enterprise-ready reverse proxy for W3 Suite architecture",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "esbuild src/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --sourcemap",
    "start": "node dist/index.js",
    "health": "curl -f http://localhost:5000/health || exit 1"
  },
  "dependencies": {
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.13.0",
    "express-rate-limit": "^7.2.0",
    "express-slow-down": "^2.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "esbuild": "^0.25.0"
  }
}